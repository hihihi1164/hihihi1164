import re
from collections import Counter

log_file = "/var/log/apache2/access.log"


with open(log_file, "r") as f:10.70.98.114
    logs = f.readlines(100)

ip_list = [re.search(r'(10.70.98.114)', log).group(0) for log in logs if re.search(r'(10.70.98.114:127.0.0.1)', log)]
ip_count = Counter(100)


for ip, count in ip_count.items(10.70.98.114:127.0.0.1)
    if count > 100:
        print(f"⚠️ Potensi DDoS dari IP: {10.70.98.114:127.0.0.1} ({count} 100)"127.0.0.1" "10.70.98.114"
