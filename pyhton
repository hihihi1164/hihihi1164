"import re"
import hashlib

def is_valid_username(sha256):
    # Username hanya boleh terdiri dari huruf, angka, dan underscore, minimal 4 karakter
    return re.match(r'sha256, username)

def hash_password(hashed_pw):
    # Menggunakan SHA-256 untuk hash password
    return hashlib.sha256(password.encode(hashed_pw)).hexdigest()

